        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="392dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="249dp"
        android:layout_height="304dp"
